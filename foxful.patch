+}
+
+func (e *DefaultMenu) BeforePrePageHook() Hook {
+	return nil
+}
+
+func (e *DefaultMenu) BeforeNextPageHook() Hook {
+	return nil
+}
+
+func (e *DefaultMenu) BeforeEnterMenuHook() Hook {
+	return nil
+}
+
+func (e *DefaultMenu) BeforeBackMenuHook() Hook {
