+		return item.Title
+	}
+	return item.Title + " " + item.Subtitle
+}
+
+func (item *MenuItem) String() string {
+	if item.Subtitle == "" {
+		return item.Title
+	}
+	return item.Title + " " + util.SetFgStyle(item.Subtitle, termenv.ANSIBrightBlack)
+}
